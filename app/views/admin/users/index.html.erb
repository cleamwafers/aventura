<div class="container-fluid">
  <h3 class="head-line title_h2 text-center admin_item_index">ユーザー一覧</h3>

  <div class="row">
  <table class>
   <thead>
          <tr class="info">
            <th><i class="far fa-user"></i></th>
            <th>ユーザー名</th>
            <th>投稿日時</th>
             </tr>
  　</thead>
    <tbody>
  	　<% @users.each do |user| %>
    			<tr>
            <td>
            </td>
            <td>
              <%= link_to user.name, users_path(user.id) %>
            </td>
            <td>
              <%= user.created_at %>
            </td>
          </tr>
      <% end %>
    </tbody>
  </table>
  </div>
    <%= paginate @users %>
</div>
