<h2>タグ編集</h2>

<%= form_with model: @tags, url: admin_tag_path do |f| %>
<div class="container">
  <table class="table">
    <tr>
      <td>
        タグ名
      </td>
      <td>
        <%= f.text_field :tags_name %>
      </td>
      <td>
        <%= f.submit "変更を保存", class: "btn btn-success" %>
      </td>
    </tr>
  </table>
</div>
<% end %>
